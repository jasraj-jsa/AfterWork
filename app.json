{
	"expo": {
		"name": "AfterWork",
		"slug": "afterwork",
		"version": "1.0.0",
		"orientation": "portrait",
		"icon": "./assets/images/logo.png",
		"scheme": "afterwork",
		"userInterfaceStyle": "automatic",
		"splash": {
			"resizeMode": "contain",
			"backgroundColor": "#ffffff"
		},
		"ios": {
			"supportsTablet": true,
			"package": "com.afterwork.app"
		},
		"android": {
			"adaptiveIcon": {
				"backgroundColor": "#ffffff"
			}
		},
		"web": {
			"bundler": "metro",
			"output": "static"
		},
		"plugins": ["expo-router"],
		"experiments": {
			"typedRoutes": true
		}
	}
}
